{
  "name": "@acala-network/api-augment",
  "version": "0.2400.0",
  "type": "module",
  "description": "Acala types augment for @polkadot/api",
  "license": "GPL-3.0-only",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "",
    "directory": "typescript-api"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "generate": "npm run load:meta && npm run generate:defs && npm run generate:meta",
    "postgenerate": "npm run pretty",
    "load:meta": "npm run load:meta:mandala && npm run load:meta:karura && npm run load:meta:acala",
    "load:meta:local": "curl -s -H \"Content-Type: application/json\" -d '{\"id\":\"1\", \"jsonrpc\":\"2.0\", \"method\": \"state_getMetadata\", \"params\":[]}' http://localhost:9944 > metadata-mandala.json",
    "load:meta:mandala": "curl -s -H \"Content-Type: application/json\" -d '{\"id\":\"1\", \"jsonrpc\":\"2.0\", \"method\": \"state_getMetadata\", \"params\":[]}' https://mandala-tc9-rpc.aca-staging.network > metadata-mandala.json",
    "load:meta:karura": "curl -s -H \"Content-Type: application/json\" -d '{\"id\":\"1\", \"jsonrpc\":\"2.0\", \"method\": \"state_getMetadata\", \"params\":[]}' https://karura-rpc.aca-api.network > metadata-karura.json",
    "load:meta:acala": "curl -s -H \"Content-Type: application/json\" -d '{\"id\":\"1\", \"jsonrpc\":\"2.0\", \"method\": \"state_getMetadata\", \"params\":[]}' https://acala-rpc.aca-api.network > metadata-acala.json",
    "generate:defs": "npm run generate:defs:mandala && npm run generate:defs:karura && npm run generate:defs:acala",
    "generate:defs:mandala": "node --experimental-specifier-resolution=node --loader ts-node/esm  node_modules/.bin/polkadot-types-from-defs --package @acala/api-augment/mandala/interfaces --input ./src/mandala/interfaces --endpoint ./metadata-mandala.json",
    "generate:defs:karura": "node --experimental-specifier-resolution=node --loader ts-node/esm  node_modules/.bin/polkadot-types-from-defs --package @acala/api-augment/karura/interfaces --input ./src/karura/interfaces --endpoint ./metadata-karura.json",
    "generate:defs:acala": "node --experimental-specifier-resolution=node --loader ts-node/esm  node_modules/.bin/polkadot-types-from-defs --package @acala/api-augment/acala/interfaces --input ./src/acala/interfaces --endpoint ./metadata-acala.json",
    "generate:meta": "npm run generate:meta:mandala && npm run generate:meta:karura && npm run generate:meta:acala",
    "generate:meta:mandala": "node --experimental-specifier-resolution=node --loader ts-node/esm node_modules/.bin/polkadot-types-from-chain --endpoint ./metadata-mandala.json --package @acala/api-augment/acala/interfaces --output ./src/mandala/interfaces",
    "generate:meta:karura": "node --experimental-specifier-resolution=node --loader ts-node/esm  node_modules/.bin/polkadot-types-from-chain --endpoint ./metadata-karura.json --package @acala/api-augment/acala/interfaces --output ./src/karura/interfaces",
    "generate:meta:acala": "node --experimental-specifier-resolution=node --loader ts-node/esm node_modules/.bin/polkadot-types-from-chain --endpoint ./metadata-acala.json --package @acala/api-augment/acala/interfaces --output ./src/acala/interfaces",
    "prebuild": "rimraf build",
    "build": "tsc -b --verbose",
    "postbuild": "node --experimental-specifier-resolution=node --loader ts-node/esm ./scripts/postbuild.js",
    "publish": "cd build && npm publish",
    "deploy": "npm run generate && npm run build && npm run publish",
    "pretty": "prettier --write --ignore-unknown --plugin prettier-plugin-jsdoc 'src/**/*' 'scripts/**/*'"
  },
  "main": "./index.cjs",
  "module": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./index.cjs",
      "default": "./index.js"
    },
    "./acala": {
      "types": "./index.d.ts",
      "require": "./index.cjs",
      "default": "./index.js"
    },
    "./karura": {
      "types": "./karura/index.d.ts",
      "require": "./karura/index.cjs",
      "default": "./karura/index.js"
    },
    "./mandala": {
      "types": "./mandala/index.d.ts",
      "require": "./mandala/index.cjs",
      "default": "./mandala/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "acala": [
        "index.d.ts"
      ],
      "karura": [
        "karura/index.d.ts"
      ],
      "mandala": [
        "mandala/index.d.ts"
      ]
    }
  },
  "keywords": [
    "acala",
    "karura",
    "types",
    "polkadot",
    "api"
  ],
  "devDependencies": {
    "@polkadot/api": "^10.9.1",
    "@polkadot/typegen": "^10.9.1",
    "prettier": "^2.7.1",
    "prettier-plugin-jsdoc": "^0.3.38",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  },
  "overrides": {
    "@polkadot/api": "$@polkadot/api",
    "@polkadot/api-derive": "$@polkadot/api",
    "@polkadot/api-augment": "$@polkadot/api",
    "@polkadot/api-base": "$@polkadot/api",
    "@polkadot/rpc-augment": "$@polkadot/api",
    "@polkadot/rpc-core": "$@polkadot/api",
    "@polkadot/rpc-provider": "$@polkadot/api",
    "@polkadot/types-create": "$@polkadot/api",
    "@polkadot/types-augment": "$@polkadot/api",
    "@polkadot/types-support": "$@polkadot/api",
    "@polkadot/types-codec": "$@polkadot/api",
    "@polkadot/types": "$@polkadot/api",
    "@polkadot/types-known": "$@polkadot/api"
  }
}
